@model E_Project.Models.Data.AssociatedNGOs

@{
    ViewData["Title"] = "AddNGOs";
}

<h1 class="text-center fst-italic">AddNGOs</h1>


@* <h4 class="text-center fw-normal text-muted">Programmes>AssociatedNGOs</h4> *@
<hr />
<div class="row justify-content-center">
    <div class="col-md-4">
        <form asp-action="AddNGOs" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NGOName" class="control-label"></label>
                <input asp-for="NGOName" class="form-control" />
                <span asp-validation-for="NGOName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NGOEmail" class="control-label"></label>
                <input asp-for="NGOEmail" class="form-control" />
                <span asp-validation-for="NGOEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NGOContact" class="control-label"></label>
                <input asp-for="NGOContact" class="form-control" />
                <span asp-validation-for="NGOContact" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success mt-2" /> |
                <a asp-action="Index" class="btn btn-primary mt-2">Back to List</a>
            </div>
            <div>
                
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
